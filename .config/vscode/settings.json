{
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "block",
  "editor.fontFamily": "CaskaydiaCove Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 21,
  "editor.fontWeight": "normal",
  "editor.letterSpacing": 0.3,
  "editor.lineHeight": 24,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    50,
    80,
    100,
    120
  ],
  "explorer.sortOrder": "default",
  "files.associations": {
    ".env": "ini"
  },
  // Codium use their own update server, but Iâ€™d rather
  // See https://github.com/VSCodium/vscodium/blob/master/DOCS.md#replacements-to-microsoft-online-services for more
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "git.autofetch": false,
  // Maybe disable this as the setting suggests it
  // could potentially download resources using HTTP
  "json.schemaDownload.enable": true,
  "npm.fetchOnlinePackageInfo": false,
  "telemetry.telemetryLevel": "off",
  "update.mode": "manual",
  "update.showReleaseNotes": false,
  // This is the only way to increase the font size of the file explorer
  "window.zoomLevel": 1.2,
  "workbench.colorTheme": "Bearded Theme Black & Emerald",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.enableExperiments": false,
  "workbench.experimental.editSessions.autoStore": "off",
  "workbench.experimental.editSessions.enabled": false,
  "workbench.iconTheme": "bearded-icons",
  // Disable local history (applicable starting v1.66 and up)
  // See https://code.visualstudio.com/updates/v1_66#_local-history for more
  "workbench.localHistory.enabled": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 20
}
